{
  "name": "kind-of-extra",
  "version": "1.0.4",
  "description": "Additional functionality to [kind-of][] type check utility. Support promises, generators, streams, errors.",
  "repository": "tunnckoCore/kind-of-extra",
  "author": "Charlike Mike Reagent <@tunnckoCore> (http://www.tunnckocore.tk)",
  "precommit.silent": true,
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "standard --verbose",
    "pretest": "npm run lint",
    "test": "npm run coverage",
    "posttest": "npm run lint:coverage",
    "coverage": "nyc node test.js",
    "lint:coverage": "nyc check-coverage --lines 0 --branches 0 --statements 0 --functions 0",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "prerelease": "npm test",
    "release": "standard-version --sign --no-verify",
    "precommit": "git add --all",
    "commit": "git-cz"
  },
  "dependencies": {
    "is-es6-generators": "^1.0.2",
    "is-node-stream": "^1.0.0",
    "is-promise": "^2.1.0",
    "kind-of": "^3.0.4",
    "lazy-cache": "^2.0.1"
  },
  "devDependencies": {
    "commitizen": "^2.8.6",
    "coveralls": "^2.11.14",
    "cz-conventional-changelog": "^1.2.0",
    "mukla": "^0.4.5",
    "nyc": "^8.3.2",
    "pre-commit": "^1.1.3",
    "standard": "^8.5.0",
    "standard-version": "^8.0.1",
    "through2": "^2.0.1"
  },
  "files": [
    "index.js",
    "utils.js"
  ],
  "keywords": [
    "check",
    "err",
    "error",
    "extra",
    "function",
    "generator",
    "generators",
    "get",
    "kind",
    "kind-of-extra",
    "kindof",
    "normalize",
    "of",
    "promise",
    "stream",
    "streams",
    "support",
    "type",
    "typecheck",
    "typeof",
    "util",
    "utility",
    "validate",
    "validation"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "verb": {
    "run": true,
    "toc": true,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "kind-of-types",
        "is-kindof",
        "to-callback",
        "always-done",
        "try-catch-core",
        "minibase",
        "mukla"
      ]
    },
    "reflinks": [
      "always-done",
      "async-done",
      "base",
      "dezalgo",
      "kind-of",
      "once",
      "rfpify"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
